<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RippleSpreadView">
        <attr name="rsv_innerCircleColor" format="color"/>
        <attr name="rsv_innerSize" format="dimension"/>
        <attr name="rsv_innerAnimDuration" format="integer"/>
        <attr name="rsv_outCircleColor" format="color"/>
        <attr name="rsv_outSize" format="dimension"/>
        <attr name="rsv_outAnimDuration" format="integer"/>
    </declare-styleable>

    <declare-styleable name="WaterRippleView">
        <attr name="rippleSpacing" format="dimension|reference"/>
        <attr name="rippleCount" format="integer|reference"/>
        <attr name="rippleColor" format="color"/>
        <attr name="rippleCenterIcon" format="reference"/>
        <attr name="rippleAutoRunning" format="boolean"/>
    </declare-styleable>
</resources>